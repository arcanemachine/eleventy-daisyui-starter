{
  "name": "eleventy-daisyui-starter",
  "version": "0.1.0",
  "description": "A starter kit for using Eleventy (11ty) with DaisyUI.",
  "scripts": {
    "start": "concurrently \"npm run tailwind-watch\" \"eleventy --serve\"",
    "tailwind": "tailwindcss -i ./src/css/tailwind-in.css -o ./src/css/tailwind-out.css --minify",
    "tailwind-watch": "tailwindcss -i ./src/css/tailwind-in.css -o ./src/css/tailwind-out.css --watch --minify",
    "build": "npm run tailwind && eleventy"
  },
  "keywords": [
    "11ty",
    "daisyui",
    "eleventy",
    "tailwind",
    "tailwindcss"
  ],
  "author": "arcanemachine",
  "license": "MIT-0",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "concurrently": "^8.2.0",
    "daisyui": "^3.0.22",
    "tailwindcss": "^3.3.2"
  }
}
